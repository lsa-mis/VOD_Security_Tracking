<% Notification.all.each do |nt| %>
    <% if (nt.opendate..nt.closedate).cover? DateTime.now %>
      <% if nt.notetype == "notice"%>
        <div class="bg-gray-200 border border-yellow-600 mt-4 p-2" >
      <% else %>
        <div class="bg-yellow-200 border border-red-800 mt-4 p-2 flex wrap space-between" >
          <div class="mr-2">
            <%= render :partial => 'svg/alert' %>
          </div>
      <% end %>
          <div>
            Starting: <%= (nt.opendate).strftime("%a, %b %-d, %Y at %l:%M %p") %>
            <br>Ending: <%= (nt.closedate).strftime("%a, %b %-d, %Y at %l:%M %p") %>
            <br><%= nt.note %>
          </div>
        </div> 
    <% end %>
  <% end %>