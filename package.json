  },
  "version": "0.1.0",
  "devDependencies": {
    "esbuild": "^0.25.0",
    "postcss": "^8.4.49",
    "sass": "^1.83.1"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --outdir=app/assets/builds --public-path=/assets",
